<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="PrimeraAppNetMaui.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:PrimeraAppNetMaui"
    xmlns:pages="clr-namespace:PrimeraAppNetMaui.Pages"
    Shell.FlyoutBehavior="Flyout"
    Title="PrimeraAppNetMaui">

    <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate pages:MainPage}"
        Route="MainPage" />
    <ShellContent
        Title="Primera Pagina"
        ContentTemplate="{DataTemplate pages:PrimeraPagina}"
        Route="PrimeraPagina" />
    <ShellContent
    Title="Primera Pagina"
    ContentTemplate="{DataTemplate pages:Controles1Page}"
    Route="PrimeraPagina" />
        

</Shell>
